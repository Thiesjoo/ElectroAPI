{
  "name": "electroapi",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "predocker:test": "docker build --target testing -t thiesjoo/electroapi-test:latest .",
    "docker:test": "docker run thiesjoo/electroapi-test:latest",
    "predocker:start:prod": "docker build --target production -t thiesjoo/electroapi:latest .",
    "docker:start:prod": "docker run -v ${HOST_PROJECT_PATH}/env.prod.yml:/usr/src/env.prod.yml -p 8081:8081 -e CONFIG_PATH='../env.prod.yml' thiesjoo/electroapi:latest",
    "compose:start": "docker-compose up --build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gendocs": "npx @compodoc/compodoc -p tsconfig.json -s "
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.2",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/platform-socket.io": "^7.6.11",
    "@nestjs/swagger": "^4.7.11",
    "@nestjs/websockets": "^7.6.11",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "joi": "^17.3.0",
    "js-yaml": "^4.0.0",
    "mongoose": "^5.11.12",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "mongoose-paginate-v2": "^1.3.13",
    "ms": "^2.1.3",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2-refresh": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "socket.io": "^3.1.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^14.14.22",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleDirectories": [
      ".",
      "src",
      "src/util",
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
